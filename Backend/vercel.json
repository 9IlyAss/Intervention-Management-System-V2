{
  "version": 2,
  "builds": [
    {
      "src": "Backend/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "Backend/models/**",
          "Backend/Routes/**",
          "Backend/middleware/**",
          "Backend/config/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Backend/server.js"
    }
  ]
}